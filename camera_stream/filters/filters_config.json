{
  "filters": {
    "horizontal_flip": {
      "enabled": true,
      "description": "Flip the frame horizontally"
    },
    "minimize_colors": {
      "enabled": true,
      "description": "Minimize the number of colors in the frame",
      "parameters": {
        "color_levels": 42
      }
    },
    "zoom_in_effect": {
      "enabled": true,
      "description": "Zoom in on the frame",
      "parameters": {
        "max_snapshots": 10,
        "scale_speed": 0.1,
        "max_scale": 3.0,
        "opacity": 0.9
      }
    }
  }
}